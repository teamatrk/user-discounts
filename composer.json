{
    "name": "teamatrk/user-discounts",
    "description": "Reusable Laravel package for user-level discounts",
    "version":"1.0",
    "type": "library",
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "teamatrk\\UserDiscounts\\": "src/",
            "teamatrk\\UserDiscounts\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "teamatrk\\UserDiscounts\\DiscountServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
